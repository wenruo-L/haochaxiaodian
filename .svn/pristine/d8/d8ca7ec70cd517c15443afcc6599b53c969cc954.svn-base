.bg {
  width: 100%;
  height: 100%;
  position: relative;
}

.bgimg {
  width: 100%;
  display: block
}

.btn {
  position: absolute;
  left: 0;
  bottom: 3%;
  width: 100%;
  line-height: 100rpx;
  height: 100rpx;
}

.text {
  background: #158e57;
  width: 80%;
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  color: #fff;
  font-size: 34rpx;
  margin: 0 auto;
  text-align: center;
  font-weight: 500;
}

.share_good_poster {
  width: 100%;
