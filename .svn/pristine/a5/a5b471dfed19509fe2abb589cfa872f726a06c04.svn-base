/**全局page基本样式5*/

page {
  background: #f5f5f5;
  height: 100%;
  width: 100%;
  -webkit-text-size-adjust: none;
  padding: 0;
  margin: 0;
  font-size: 13px;
}
 
 
 
.fl{ float: left}
.fr{ float: right}
.clear{clear:both;} 
.clearfix:after{
	content: "";
	display: block;
	clear: both;
}

 
.clearfix:after {
  content: "";
  display: block;
  clear: both;
}
 
.maincolor{
  color: #0E6D45;
}
.mezzo_color{
  color: #666;
}
.shallow_color{
  color: #999;
}

.top_title_font{
   font-size: 40rpx;
}
.one_shopname_font{
  font-size: 36rpx;
}
