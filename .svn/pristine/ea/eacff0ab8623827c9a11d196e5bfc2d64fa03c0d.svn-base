
<scroll-view scroll-x="true" class="tab-h" scroll-left="{{scrollLeft}}">
  <view class='flex'>
    <view class="tab-item {{currentTab==index?'active':''}}" wx:for="{{h_text}}" data-current="{{index}}" catchtap="swichNav" wx:key="this">
      <text>{{item.text}}</text>

    </view>

  </view>
</scroll-view>

