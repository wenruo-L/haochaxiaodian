/* pageA/pages/share_pages/share_pages.wxss */
.content{
  background: #fff;
}
.box{
  padding-top: 50rpx;
}
.hearder_img{
  width: 120rpx;
  height: 120rpx;
  border-radius:50%;
  margin: 0 auto;
  background: skyblue; 
}
.hearder_img>image{
  width: 100%;
  height: 100%;
  border-radius:50%; 
}
.good_detail_out{
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
}
.good_detail{
  width: 96%;
  height: auto;
  margin: 0 auto;
  background: #f5f5f5;
}
.good_detail_in{
  width: 96%;
  height: auto;
  margin: 0 auto;
}
.good_title{
  width: 96%;
  margin: 0 auto;
  text-align: left;
  padding: 20rpx 0;
  border-bottom:1px solid#e6e6e6;  
}
.good_sum{
  width: 96%;
  margin: 0 auto;
  padding: 20rpx;
}
.goodimg{
  width: 144rpx;
  height: 144rpx;

}
.goodimg>image{
  width: 100%;
  height: 100%;
}
.good_name_wrap{
  width: 75%;
  height: 140rpx;
  margin-left: 160rpx;
  position: relative;
}
.good_name{
  width: 100%;
  height: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
}
.price{
  position: absolute;
  left: 0;
  bottom: 0;
}
.waitForPayTimer{
  width: 100%;
  padding: 50rpx 0 30rpx 0; 
  text-align: center;
}
.waitForPayTimer>text{
  width: 70rpx;
  height: 70rpx;
  padding: 10rpx;
  line-height: 70rpx;
  color: #fff;
  font-size: 36rpx;
  border-radius:8rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  margin-bottom: 20rpx; 
  background: #000;
}
.timerTips{
  width: 100%;
  color: #999;
  text-align: center;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.payNow{
  width: 450rpx;
  height: 88rpx;
  border-radius:8rpx;
  background: #0E6D45;
  line-height: 88rpx;
  color: #fff;
  text-align: center;
  margin:  0 auto;
  margin-top: 50rpx; 
}
.payNowTips{
    width: 100%;
  color: #999;
  text-align: center;
  font-size: 24rpx;
  margin-top: 20rpx;
  padding-bottom: 50rpx;
}
