/* pages/address/address.wxss */

.item {
  align-items: flex-start;
  background: #fff;
  height: 160rpx;
  display: flex;
  border-top: 0.4px solid #dedede;
  border-bottom: 0.4px solid #dedede;
  margin-bottom: 15rpx;
  padding: 20rpx;
  flex-direction: column;
}

.line-vertical {
  width: 100%;
  height: 3px;
  background: #dedede;
  -webkit-transform: scaleY(0.1);
  -webkit-transform-origin: 0 0;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
}

.t_button {
  width: 90rpx;
  height: 30px;
  align-items: center;
  border: 0.4px solid #dedede;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  padding-right: 20rpx;
  padding-left: 20rpx;
  margin-right: 20rpx;
  border-radius: 5px;
}

.radio {
  -webkit-transform: scale(0.7);
  -webkit-transform-origin: 0 0;
}
