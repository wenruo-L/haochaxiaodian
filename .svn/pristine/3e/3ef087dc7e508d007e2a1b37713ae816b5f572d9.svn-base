/* pages/pinhaocha/pinhaocha.wxss */
.fl{ float: left}
.fr{ float: right}
.clear{clear:both;} 
.clearfix:after{
	content: "";
	display: block;
	clear: both;
}
.empty_div{
  width: 100%;
  height: 20rpx;
  background: #f5f5f5;
}
.index_top_mind{
  width: 100%;
  height:100%;
  /* background: skyblue; */
  position: relative;
}
.index_top_bg{
  width: 100%;
  height: 159px;
}
.index_top_bg>image{
    width: 100%;
    height: 100%;
}
.group_booking_detail{
  width: 100%;
  height: 100%;
  border-top: 1px solid #e6e6e6;
  background: #fff;
}
.group_booking_detail_in{
  width: 96%;
  height: 100%;
  margin: 0 auto;
}
.group_booking{
  width: 100%;
 padding: 40rpx 0;
 /* background: skyblue;  */
}
.group_booking_good_img{
  width: 215rpx;
  height: 215rpx;
  display: inline-block;
  vertical-align: bottom;
}
.group_booking_good_img>image{
  width: 100%;
  height: 100%;
  border-radius:3px; 
}
.group_booking_good_detail{
  width: 67%;
  display: inline-block;
  vertical-align: bottom;
  margin-left: 20rpx;
}
.group_booking_good_title{
  font-size: 30rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
  line-height: 49rpx;
}
.group_booking_price{
   display: inline-block;
  font-size: 24rpx;
}
.group_booking_price>text{
  color: #0E6D45;
}
.own_price{
  display: inline-block;
  color: #999999;
  font-size: 20rpx;
  text-decoration: line-through;
  margin-left: 10rpx;
}
.group_booking_3{
  width: 100%;
  margin-top:66rpx; 
}
.to_group_booking{
  display: inline-block;
}
.user_header{
   display: inline-block;
}
.group_booking_number{
  color: #999999;
  font-size: 24rpx;
 display: inline-block;
 vertical-align: middle;
}
.header_img{
  width: 48rpx;
  height: 48rpx;
  display: inline-block;
  vertical-align: middle;
}
.header_img>image{
  width: 100%;
  height: 100%;
}
.btn_to_gb{
  vertical-align: middle;
  font-size: 26rpx;
  color: #fff;
  width: 144rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  display: inline-block;
  background: #0E6D45;
  border-radius:50px; 
  margin-left: 20rpx;
}
.header_img_turn{
  margin-left: -15rpx;
}