/* pages/fast_order/fast_order.wxss */
.noMore{
  width: 100%;
  height: auto;
  text-align: center;
  color: #666;
  margin-top: 50rpx;
  padding-bottom: 70rpx;
}
.noMoreTipsimg{
  width: 360rpx;
  height: 340rpx;
  margin: 0 auto;
}
.noMoreTipsimg>image{
  width: 100%;
  height: 100%;
}
.content{
  border-top:1px solid #e6e6e6; 
}
.swiper-tab-list-in{
  width: 168rpx;
  height: 82rpx;
  line-height: 82rpx;
  text-align: center;
  border-bottom:1px solid #e6e6e6; 
}
.on{
  background: #fff;
}
.swiper_content{
  background: #fff;
}
.swiper-tab{
  position: relative;
}
.swiper_content{
  border-top:1px solid #e6e6e6; 
  width: 78%;
 height: 100%;
overflow: scroll;
  position: absolute;
  left: 168rpx;
  top: 0;
} 
scroll-view{
  width: 100%;
  height: 100%;
}
.swiper_content::-webkit-scrollbar {
    display: none;
}
.good_detail_out{
  width: 100%;
  height: auto;
 overflow-x: scroll;
  background: #fff;
  /* height:2000rpx; */
}
.good_detail{
  width: 100%;
  height: auto;
  padding: 20rpx 0;
}
.good_img{
  width: 150rpx;
  height: 150rpx;
  display: inline-block;
  vertical-align: middle;
  margin-left: 20rpx;
}
.good_img>image{
  width: 100%;
  height: 100%;
}
.good_msg{
  display: inline-block;
  width: 63%;
  height: auto;
  vertical-align: middle;
   margin-left: 20rpx;
}
.good_name{
  line-height: 48rpx;
  overflow: hidden;
  /*1.超出内容要隐藏，创建边界*/
  text-overflow: ellipsis;
  /*2.文字遇到边界时，显示省略号*/
  display: -webkit-box;
  /*3.设置该容器为弹性盒子*/
  -webkit-line-clamp: 2;
  /*4.表示你的文字可以支持的最大行数*/
  -webkit-box-orient: vertical;
}
.good_sum{
  width: 100%;
  height: 100%;
  margin-top:20rpx; 
}
.sum{
  display: inline-block;
  font-weight: 600;
}
.order{
  display: inline-block;
  width: 95rpx;
  height: 40rpx;
  text-align: center;
  padding-top: 3rpx;
  color: #fff;
  border-radius:30rpx; 
}
.more_mask_out{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
 
}
.more_mask{
   background: rgba(0, 0, 0, .5);
   width: 100%;
}
.choose_detail_out{
  width: 100%;
  height: auto;
  position: absolute;
  left: 0;
  bottom: 0;
  background: #fff;
}
.choose_detail{
  width: 96%;
  height: auto;
  margin: 0 auto;
}
.choose_goodsdetail{
  width: 100%;
  height: 150rpx;
  padding: 25rpx 0;
  background: #fff;
}
.choose_goodsImg{
  width: 150rpx;
  height: 150rpx;
  display: inline-block;
  vertical-align: middle;
}
.choose_goodsImg>image{
  width: 100%;
  height: 100%;
}
.choose_goodsText{
  display: inline-block;
  vertical-align: middle;
  width: 72%;
  margin-left:20rpx;
  height: 150rpx;
}
.choose_goodsTitle{
  width: 100%;
  height: 80rpx;
  margin-top:15rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
}
.choose_goodsSum{
  color: #0E6D45;
  font-size: 30rpx;
}
.color999{
  color: #999;
}
.choose_goods_L{
  width: 50%;
  display: inline-block;
  vertical-align: middle;
}
.choose_goods{
  width: 100%;
}
.choose_goods_out {
  padding: 27rpx 0;
  border-top: 1px solid#e6e6e6; 
}
.choose_good_add_min{
  width: 320rpx;
  height: 64rpx;
  border: 1px solid#e6e6e6;
  vertical-align: middle;
}
.dis_middle{
  display: inline-block;
  vertical-align: middle;
  line-height: 65rpx;
  text-align: center;
}
.min{
  width: 65rpx;
  height: 65rpx;
  border-right:1px solid#e6e6e6; 
}
.add{
  width: 65rpx;
  height: 65rpx;
  border-left:1px solid#e6e6e6; 
}
.choose_good_inp{
  width: 188rpx;
  height:65rpx; 
}
.sum_total{
  border-top:1px solid#e6e6e6; 
}
.total{
  width: 70%;
  padding: 35rpx 0;
  display: inline-block;
  text-align: right;
  vertical-align: middle;
}
.zongjia{
  margin-right: 26rpx;
}
.submit_num{
  width: 30%;
  padding: 35rpx 0;
  display: inline-block;
  background: #0e6d45;
  text-align: center;
  color: #fff;
  vertical-align: middle;
}
