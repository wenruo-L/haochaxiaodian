/* pageA/pages/rankings/rankings.wxss */
page{
  width: 100%;
  height: 100%;
  background: #fff
}
.item {
  display: flex;
  align-content: center;
  height: 100rpx;
  padding-bottom:30rpx;
  margin: 0 20rpx;
  
}

.index {
  text-align: center;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #000;
  margin-right: 20rpx;
}
.index image{
  display: inline-block;
  width: 65rpx;
  height: 65rpx;
  vertical-align: middle;
  
}
.logo {
  display: block;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.name {
  height: 80rpx;
  line-height: 80rpx;
  width: 150rpx;
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 1;
overflow: hidden;
color: #666;
font-size: 30rpx;

}
.last{
  flex: 1;
  text-align: right;
  vertical-align: middle;
  margin-top: 10rpx;
  padding-right:20rpx; 
  /* display: flex;
   align-items:center;
  height: 100rpx;
   flex-direction: column */
}
.last .num{
  color: #EC0000;
  font-size: 30rpx;
  font-weight: 800;
}

.last .text{
  color: #999;
  font-size: 24rpx;
 
}