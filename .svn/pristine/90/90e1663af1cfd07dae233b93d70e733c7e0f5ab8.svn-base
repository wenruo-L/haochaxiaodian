<!--pageA/pages/record/record.wxml-->
<view class='list'>
  <view class='item' wx:for="{{list}}" wx:key="this">
    <!-- <view class='title'>2018-08</view> -->
    <view class='mes'>
      <view class='goodsName'>【<block wx:if="{{item.userType==0}}">分享红包】{{item.title}}</block><block wx:if="{{item.userType==1}}">下单红包】 {{item.title}}</block><block wx:if="{{item.userType==10}}">提现】提现单号:{{item.id}}</block>
      </view>
      <view class='bot'>
        <view class='left'>
        <block wx:if="{{item.userType==10}}">
         <view class='time'>
            <view class='text'>
              <text>提现时间：</text> 
              <block >{{item.updateTime}}</block>
              
            </view>
          </view>
        </block>
        <block wx:else>
          <view class='time'>
            <view class='text'>
              <text>下单时间：</text> {{item.orderTime}}
             
             
            </view>
          </view>
          <view class='time'>
            <view class='text'>
              <text>结算时间：</text> {{item.successTime}}
            </view>
          </view>
        </block>
          
        </view>
        <view class="right {{item.userType==10? 'color top':''}}">
          
           <block wx:if="{{item.userType==10}}">-￥{{item.amount/100}}</block>
              <block wx:else>+￥{{item.profit/100}}</block>
        </view>
      </view>
    </view>
