/* pages/order_detail_chakanwuliu/order_detail_chakanwuliu.wxss */
.wait_pay_out{
  width: 100%;
  height: auto;
}
.wait_pay{
  width: 94%;
  margin: 0 auto;
  padding: 60rpx 0;
  position: relative;
}
.wait_pay_text{
  color: #fff;
  display: inline-block;
  vertical-align: middle;
}
.wait_pay_icon{
  width: 140rpx;
  height: 130rpx;
  display: inline-block;
  vertical-align: middle;
  position: absolute;
  right: 0;
  top: 20rpx;
}
.wait_pay_icon>image{
  width: 100%;
  height: 100%;
}
.logistics_out{
  width: 100%;
  height: auto;
  background: #fff;
  margin-top: 20rpx;
  border-bottom: 1px solid #e6e6e6;
  border-top: 1px solid #e6e6e6;
}
.logistics{
  width: 94%;
  margin: 0 auto;
  padding: 30rpx 0 38rpx 0;
}
.logistics_name{
  color: #333;
  line-height: 48rpx;
}
.logistics_trace_out{
  width: 100%;
  height: 100%;
  background: #fff;
  margin-top: 20rpx;
  border-bottom: 1px solid #e6e6e6;
  border-top: 1px solid #e6e6e6;
}
.logistics_trace{
  width: 94%;
  margin: 0 auto;
}
.logistics_trace_title{
  width: 100%;
  text-align: left;
  line-height: 48rpx;
  padding: 30rpx 0;
}
.date_detail{
  width: 94%;
  margin: 0 auto;
}
.date{
  width: 100%;
  text-align: left;
}
.time{
  width: 93%;
  border-left: 1px solid #e6e6e6;
  padding: 17rpx 0;
  margin-left:67rpx;
  margin-top: 12rpx;
  margin-bottom: 12rpx;
  position: relative;
}
.time2{
  width: 93%;
  border-left: 1px solid #e6e6e6;
  padding-top: 17rpx;
  margin-left:67rpx;
  margin-top: 12rpx;
  margin-bottom: 12rpx;
  position: relative;
}
.s_dot{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  left: -12rpx;
  top: 24rpx;
}
.s_dot1{
  width: 14rpx;
  height: 14rpx;
  position: absolute;
  left: -8rpx;
  top: 20rpx;
}
.s_dot1>image{
  width: 100%;
  height: 100%;
}
.s_dot2{
  width: 14rpx;
  height: 14rpx;
  position: absolute;
  left: -8rpx;
  top: 67rpx;
}
.s_dot2>image{
  width: 100%;
  height: 100%;
}
.s_dot3{
  width: 14rpx;
  height: 14rpx;
  position: absolute;
  left: -8rpx;
  bottom: 48rpx;
}
.s_dot3>image{
  width: 100%;
  height: 100%;
}
.s_dot>image{
  width: 100%;
  height: 100%; 
}
.exact_time{
  display: inline-block;
  vertical-align: top;
  margin-left: 38rpx;
  line-height: 48rpx;
}
.logistics_detail{
   display: inline-block;
  vertical-align: top;
  margin-left: 23rpx;
  width: 76%;
  line-height: 48rpx;
}
.logistics_detail1{
  display: inline-block;
  vertical-align: top;
  margin-left: 23rpx;
  width: 76%;
  line-height: 48rpx;
  overflow: hidden;
  /*1.超出内容要隐藏，创建边界*/
  text-overflow: ellipsis;
  /*2.文字遇到边界时，显示省略号*/
  display: -webkit-box;
  /*3.设置该容器为弹性盒子*/
  -webkit-line-clamp: 2;
  /*4.表示你的文字可以支持的最大行数*/
  -webkit-box-orient: vertical;
}