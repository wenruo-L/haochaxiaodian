/* pages/ruzhu/ruzhu.wxss */
.fl{ float: left}
.fr{ float: right}
.clear{clear:both;} 
.clearfix:after{
	content: "";
	display: block;
	clear: both;
}

 
.empty_div{
  width: 100%;
  height: 20rpx;
  background: #f5f5f5;
}
.index_top_mind{
  width: 100%;
  height:100%;
  margin-bottom: 20rpx;
}
.index_top_bg{
  width: 100%;
  height: 159px;
}
.index_top_bg>image{
    width: 100%;
    height: 100%;
}
.infor_top{
  width: 100%;
  height: 100%;
}
.personal_infor_out{
  width: 100%;
  border-top:1px solid #e6e6e6; 
  background: #fff;
}
.personal_infor{
  width: 96%;
  margin: 0 auto;
  padding: 32rpx 0;
}
.infor{
  color: #666666;
  width: 50%;
  font-size: 30rpx;
  text-align: left;
}
.mation{
  font-size: 30rpx;
  
}
.mation>input{
  width: 230rpx;
  font-size: 30rpx;
  text-align: right;
  display: inline-block;
  vertical-align: middle;
}
.mation>text{
   vertical-align: middle;
}
.enter_img{
  vertical-align: middle;
  width: 16rpx;
  height: 28rpx;
  display: inline-block;
   margin-left: 20rpx;
}
.enter_img>image{
  width: 100%;
  height: 100%;
}
.boder_b_line{
  border-bottom:1px solid #e6e6e6;
}
.infor_bottom{
  margin-top: 20rpx;
}
.ruzhushenqing_out{
  width: 100%;
  padding: 20rpx 0;
}
.ruzhushenqing{
  width: 92%;
  margin: 0 auto;
}
.rezhu_btn{
  width: 100%;
  height: 88rpx;
  background: #0E6D45;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
  border-radius:5px;
  margin-top: 30rpx; 
}
.select_img{
  width: 37rpx;
  height: 37rpx;
}
.select_img>image{
  width: 100%;
  height: 100%;
}
.select_wrap{
  display: inline-block;
  vertical-align: middle;
}
.tips{
  color: #9B9B9B;
  font-size: 26rpx;
  display: inline-block;
  vertical-align: middle;
}
.tips>text{
  color: #0E6D45;
  font-size: 26rpx;
}
.pdu{
  width: 86%;
  margin: 0 auto;
}
.picker{
  display: inline-block;
  vertical-align: middle;
}