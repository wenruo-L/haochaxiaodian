/* pages/pinhaocha/pinhaocha.wxss */
.tips{
  width: 100%;
  text-align: center;
  color: #666;
  margin-top: 60rpx;
}
.fl{ float: left}
.fr{ float: right}
.clear{clear:both;} 
.clearfix:after{
	content: "";
	display: block;
	clear: both;
}
.user_header_wrap{
  width: 96rpx;
  height: 48rpx;
  overflow: hidden;
  display: inline-block;
}
.empty_div{
  width: 100%;
  height: 20rpx;
  background: #f5f5f5;
}
.index_top_mind{
  width: 100%;
  height:100%;
  /* background: skyblue; */
  position: relative;
}
.index_top_bg{
  width: 100%;
  height: 159px;
}
.index_top_bg>image{
    width: 100%;
    height: 100%;
}
.group_booking_detail{
  width: 100%;
  height: 100%;
  border-top: 1rpx solid #e6e6e6;
  background: #fff;
  /* margin-top: 20rpx; */
}
.group_booking_detail_in{
  width: 96%;
  height: 100%;
  
  margin: 0 auto;
}
.group_booking{
  width: 100%;
 
 padding: 40rpx 0;
 /* background: skyblue;  */
}
.group_booking_good_img{
  width: 215rpx;
  height: 215rpx;
  display: inline-block;
  vertical-align: bottom;
}
.group_booking_good_img>image{
  width: 100%;
  height: 100%;
  border-radius:5rpx; 
}
.group_booking_good_detail{
  position: relative;
  width: 67%;
  display: inline-block;
  vertical-align: bottom;
  margin-left: 20rpx;
}
.group_booking_good_title{
     width: 90%;
   height: auto;
  font-size: 36rpx;
  
  white-space:nowrap;
  
  line-height: 49rpx;
  margin-bottom: 10rpx;
    overflow: hidden;
  /*1.超出内容要隐藏，创建边界*/
  text-overflow: ellipsis;
  /*2.文字遇到边界时，显示省略号*/
  display: -webkit-box;
  /*3.设置该容器为弹性盒子*/
  -webkit-line-clamp: 2;
  /*4.表示你的文字可以支持的最大行数*/
  -webkit-box-orient: vertical;
}
.group_booking_price{
   display: inline-block;
  font-size: 30rpx;
}
.group_booking_price>text{
  color: #0E6D45;
}
.own_price{
  display: inline-block;
  color: #999999;
  font-size: 26rpx;
  text-decoration: line-through;
  margin-left: 10rpx;
}
.group_booking_3{
  width: 100%;
  margin-top:50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center; 
}
.to_group_booking{
  display:flex;
  height:auto;
  vertical-align:middle;
  justify-content:flex-end;
  align-items:center;

}
.user_header{
   display: inline-block;
   width: 96rpx;
   height: 48rpx;
   overflow: hidden;
   vertical-align: middle;
   right: 50rpx;
   bottom: 10rpx;
   text-align:right;
 
}
.group_booking_number{
  color: #999999;
  font-size: 24rpx;
 display: inline-block;
 vertical-align: middle;
}
.header_img{
  width: 48rpx;
  height: 48rpx;
  border-radius:50%; 
  display: inline-block;
  vertical-align: middle;
}
.header_img>image{
  width: 100%;
  height: 100%;
  border-radius:50%; 
}
.btn_to_gb{
  vertical-align: middle;
  font-size: 26rpx;
  color: #fff;
  width: 144rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  display: inline-block;
  background: #0E6D45;
  border-radius:50px; 
  margin-left: 20rpx;
}
.header_img_turn{
  margin-left: -15rpx;
}