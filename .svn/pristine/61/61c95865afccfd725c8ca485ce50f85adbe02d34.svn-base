.wrap {
  text-align: center;
  margin-top: 250rpx;
  color: #999;
}

.noMore {
  width: 380rpx;
  height: 380rpx;
  margin: 0 auto;
}

page {
  background: #fff;
}

.flex {
  display: flex;
  justify-content: center;
  border-bottom: 1px solid #ddd;
  padding: 20rpx 0;
  /* margin-bottom:20rpx; */
  background: #fff;
}

image {
  display: block;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.tis{
  line-height: 100rpx;
  flex: 1;
  text-align: right
}
.name {
  line-height: 100rpx;
  height: 100rpx;
  padding-left: 20rpx;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.time {
  line-height: 100rpx;
  width: 230rpx;
  text-align: center;
}

.btn {
  color: #fff;
  width: 144rpx;
  height: 60rpx;
  font-size: 24rpx;
  line-height: 60rpx;
  text-align: center;
  background: #0e6d45;
  margin-top: 20rpx;
  border-radius: 50rpx;
  margin-left: 20rpx;
}
