  //待付款的倒计时
  timeFormat(param) {//小于10的格式化函数
    return param < 10 ? '0' + param : param;
  },
  counttime: function () {
    var that = this;
    var starttime = that.data.kashishijian;
    var validtime = that.data.daojishi;
    var zong_time = []
    starttime.forEach(function (v, i) {
      zong_time.push(Number(v) + Number(validtime[i] * 1000))
    })
    console.log(zong_time)
    var timestamp = new Date().getTime();
    var dao = []
    zong_time.forEach(function (a, b) {
      dao.push(Number(a) - Number(timestamp))
    })
    console.log("倒计时", dao)
    dao.forEach((el, index) => {
      if (el > 0) {
        let time = parseInt(el / 1000)
        var Time = that.data.Time;
        Time.push(time)
        that.setData({
          Times: Time
        })
      }
    })
    that.data.Time.forEach((el, index) => {
      var Hours = that.data.hours;
      let Hours_num = parseInt(el / 3600);
      let mins_num = parseInt((el - (Hours_num * 3600)) / 60);
      let secs_num = parseInt(el - (Hours_num * 3600) - (mins_num * 60));
      var timers = that.data.timer;
      // console.log(that.timeFormat(Hours_num) + ":" + that.timeFormat(mins_num) + ":" + that.timeFormat(secs_num))
      let timers_num = that.timeFormat(Hours_num) + ":" + that.timeFormat(mins_num) + ":" + that.timeFormat(secs_num)
      that.data.timer.push(timers_num)
      that.setData({
        timer: that.data.timer
      })

      // Hours.push(Hours_num)
    })
    console.log(that.data.timer)
    that.data.timer.forEach((el, index) => {
      that.data.pintuan_details[index].lastTime = el;
      that.setData({
        pintuan_details: that.data.pintuan_details
      })
    })
    console.log(typeof that.data.pintuan_details[1].lastTime)
    console.log("====================pintuan_details", that.data.pintuan_details)
    // console.log(hours)
  },
