/**全局page基本样式5*/
modal{
  display: none
}
page {
  background: #f5f5f5;
  height: 100%;
  width: 100%;
  -webkit-text-size-adjust: none;
  padding: 0;
  margin: 0;
  font-size: 13px;
}

.selected_color{
  color: #FF0000;
}
.yichu{
  width: 100%;
  height: 40rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
}
.fl{ float: left}
.fr{ float: right}
.clear{clear:both;} 
.clearfix:after{
	content: "";
	display: block;
	clear: both;
}
