/* pages/fans_record/fans_record.wxss */
.fans_wrap{
  width: 100%;
  height: 100%;
}
.just_box{
  width: 100%;
  height: auto;
   margin-top: 20rpx; 
}
.fans_out{
  width: 100%;
  height: auto;
  padding: 35rpx 0 40rpx 0;
  background: #fff;
  border-top:1px solid #e6e6e6; 
}
.fans{
  width: 96%;
  height: 100%;
  margin: 0 auto;
  position: relative;
}
.fans_header{
  width: 80rpx;
  height: 80rpx;
  border-radius:50%; 
  display: inline-block;
  font-size: 0;
  vertical-align: middle;
}
.fans_header>image{
  width: 100%;
  height: 100%;
  border-radius:50%; 
}
.fans_name{
  width: 68%;
  font-size: 30rpx;
  color: #666;
  display: inline-block;
  vertical-align: middle;
  padding-left: 40rpx;
}
.time{
  margin-top: 20rpx;
  color: #666;
  font-size: 30rpx;
  text-align: right;
  display: inline-block;
  vertical-align: middle;
}