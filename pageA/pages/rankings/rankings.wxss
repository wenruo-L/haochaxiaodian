/* pageA/pages/rankings/rankings.wxss */
page{
  width: 100%;
  height: 100%;
 background: none
}
.f5{
   background: #f5f5f5;
   padding: 20rpx 0;
}

.f5 .item{
  margin: 0;
  
   /* padding: 20rpx 20rpx 0 20rpx; */
}
.item {
  display: flex;
  align-content: center;
  height: 100rpx;
 
 
  background: #fff;
  padding: 20rpx 20rpx 0 20rpx;
  border-bottom: 1px solid #eee
}

.index {
  text-align: center;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #000;
  margin-right: 20rpx;
}
.index image{
  display: inline-block;
  width: 65rpx;
  height: 65rpx;
  vertical-align: middle;
  
}
.logo {
  display: block;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.name {
  height: 80rpx;
  line-height: 80rpx;
  width: 150rpx;
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 1;
overflow: hidden;
color: #666;
font-size: 30rpx;

}
.last{
  flex: 1;
