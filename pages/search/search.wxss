/* pages/sreach/sreach.wxss */
.content{
  width: 100%;
  background: #fff;
}
.sreach_input_out{
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: flex-start;
}
.sreach_input{
  width: 85%;
  height: 100%;
  margin-left: 20rpx;
  background: #f5f5f5;
  border-radius:50rpx; 
  display: flex;
  justify-content: flex-start;
}
.searchBtn{
  width: 15%;
  height: 100%;
  line-height: 50rpx;
  text-align: center;
  color: #666;
}
.sreach_input>image{
  width: 40rpx;
  height: 40rpx;
  margin-top: 5rpx;
  margin-left: 20rpx;
}
.sreach_value{
  width: 85%;
  height: 100%;
  border: 0;
}
.sreach_value{
  padding-left: 10rpx;
}
.search_record_out{
  width: 100%;
  height: 50rpx;
  margin-top: 10rpx;
  border-top:1px solid#e6e6e6 
}
.search_record{
  width: 93%;
  height: 100%;
  margin: 0 auto;
}
.no_record{
  width: 100%;
  height: 50rpx;
  padding-top:15rpx;
  font-size: 26rpx; 
  display: inline-block;
}
.record{
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.record_item{
  height: 50rpx;
  padding: 5rpx 20rpx;
  background: #f5f5f5;
  font-size: 26rpx;
  line-height: 46rpx;
  text-align: center;
  border-radius:50rpx; 
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.record_in{
  width: 93%;
  margin: 0 auto;
  height: 50rpx;
  text-align: left;
  position: relative;
}
.clear_record{
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  position: absolute;
  right: 0;
  top: 10rpx;
}
.clear_record>image{
  width: 100%;
  height: 100%;
}
.to_search_result_item{
  width: 93%;
  height: 70rpx;
  margin: 0 auto;
  position: relative;
}
.to_search_result_item_text{
  height: 100%;
  line-height: 70rpx;
}
.rightEnter{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 0;
  top: 10rpx;
}
.rightEnter>image{
  width: 100%;
  height: 100%;
}
.line{
  width: 100%;
  height: 1px;
  background: #e6e6e6;
}
.to_search_result{
  width: 100%;
  height: 142rpx;
  margin-top: 10rpx;
  border-top:1px solid#e6e6e6; 
}